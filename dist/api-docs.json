{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "zentomic Notifier Node on Heroku",
        "description": "Zentomic Notifier Node, source code (https://github.com/Zentomic/zentomic-notifier-node)\n"
    },
    "host": "zentomic-notifier-node.herokuapp.com",
    "basePath": "",
    "schemes": [
        "https"
    ],
    "consumes": [
        "application/json",
        "text/xml"
    ],
    "produces": [
        "application/json",
        "text/html"
    ],
    "paths": {
        "/signup-local": {
            "get": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local user",
                        "required": true
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "type": "string",
                        "description": "password of local user",
                        "required": true
                    },
                    {
                        "name": "fullname",
                        "in": "query",
                        "type": "string",
                        "description": "fullname of local user",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "signup with local account"
                    }
                }
            }
        },
        "/login-local": {
            "get": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local user",
                        "required": true
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "type": "string",
                        "description": "password of local user",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "login with local account"
                    }
                }
            }
        },
        "/auth/google/callback": {
            "get": {
                "parameters": [
                    {
                        "name": "code",
                        "in": "query",
                        "type": "string",
                        "description": "code from google plus auth service",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "login with local account"
                    }
                }
            }
        },
        "/get_agent": {
            "get": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "agent infor"
                    }
                }
            }
        },
        "/update_agent": {
            "put": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent",
                        "required": true
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "type": "string",
                        "description": "password of local agent",
                        "required": true
                    },
                    {
                        "name": "fullname",
                        "in": "query",
                        "type": "string",
                        "description": "fullname of local agent",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "err as well as agent infor"
                    }
                }
            }
        },
        "/delete_agent": {
            "delete": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent to be deleted",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "agent email and error"
                    }
                }
            }
        },
        "/create_notifier": {
            "post": {
                "parameters": [
                    {
                        "name": "fromuser",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent",
                        "required": true
                    },
                    {
                        "name": "fullname",
                        "in": "query",
                        "type": "string",
                        "description": "fullname of notifier",
                        "required": true
                    },
                    {
                        "name": "fone",
                        "in": "query",
                        "type": "string",
                        "description": "fone of notifier",
                        "required": true
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent",
                        "required": true
                    },
                    {
                        "name": "type",
                        "in": "query",
                        "type": "string",
                        "description": "type of notifier",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "err as well as agent infor"
                    }
                }
            }
        },
        "/get_notifier": {
            "get": {
                "parameters": [
                    {
                        "name": "fromuser",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "get list of notifier based on agent email"
                    }
                }
            }
        },
        "/delete_notifier": {
            "delete": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of notifier",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "notifier email and error"
                    }
                }
            }
        },
        "/SMS": {
            "get": {
                "parameters": [
                    {
                        "name": "tofone",
                        "in": "query",
                        "type": "string",
                        "description": "email of notifier",
                        "required": true
                    },
                    {
                        "name": "message",
                        "in": "query",
                        "type": "string",
                        "description": "message for sending",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "send sms test to tofone"
                    }
                }
            }
        },
        "/checkin": {
            "get": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent for creating checking",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "email and status with checkin string"
                    }
                }
            }
        },
        "/checkout": {
            "get": {
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "type": "string",
                        "description": "email of local agent for creating checkout",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "email and status with checkin string"
                    }
                }
            }
        }
    },
    "definitions": {
        "Pet": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "birthday": {
                    "type": "integer",
                    "format": "int32"
                }
            }
        },
        "Agent": {
            "type": "object",
            "properties": {
                "local": {
                    "type": "string"
                },
                "facebook": {
                    "type": "string"
                },
                "google": {
                    "type": "string"
                }
            }
        },
        "AgentSetting": {
            "type": "object",
            "properties": {
                "AgentEmail": {
                    "type": "string"
                },
                "Setting": {
                    "type": "string"
                }
            }
        },
        "message": {
            "type": "object",
            "properties": {
                "FromUser": {
                    "type": "string"
                },
                "ToUser": {
                    "type": "string"
                },
                "Message": {
                    "type": "string"
                },
                "AtDateTime": {
                    "type": "string"
                }
            }
        },
        "Notifier": {
            "type": "object",
            "properties": {
                "FromAgent": {
                    "type": "string"
                },
                "Type": {
                    "type": "string"
                },
                "Info": {
                    "type": "string"
                },
                "local": {
                    "type": "string"
                },
                "facebook": {
                    "type": "string"
                },
                "google": {
                    "type": "string"
                }
            }
        }
    }
}
